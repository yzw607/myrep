<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.wmp.common.bean.SysTelNumber" table="sys_tel_number">
        <id name="port" type="string" column="port" length="50">
        </id>
        <property name="sysTel" type="string" column="sys_tel" length="50"></property>
    </class>
</hibernate-mapping>
