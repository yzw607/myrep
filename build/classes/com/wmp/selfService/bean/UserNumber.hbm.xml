<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.wmp.selfService.bean.UserNumber" table="user_number">
        <id name="numberId" type="java.lang.Integer" column="number_id">
          <generator class="increment"></generator>
        </id>
        <property name="number" type="string" column="number" length="10"></property>
        <property name="numberType" type="string" column="number_type" length="2"></property>
        <property name="userCode" type="string" column="user_code" length="50"></property>
        <property name="port" type="string" column="port" length="50"></property>
        <property name="sysTel" type="string" column="sys_tel" length="50"></property>
    </class>
</hibernate-mapping>
