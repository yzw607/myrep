	<package name="agents" extends="struts-default">
	    <interceptors>
	        <interceptor name="adminInterceptor" class="com.wmp.common.interceptor.AdminInterceptor"/>
	        <interceptor-stack name="adminDefault">
	            <interceptor-ref name="adminInterceptor"/>
	            <interceptor-ref name="defaultStack" />
	        </interceptor-stack>
	    </interceptors>
	    <default-interceptor-ref name="adminDefault" />
	    <global-results>
            <result name="login">/404.jsp</result>
        </global-results>
        
		<action name="agentsList" class="agentsManageAction" method="agentsList">
			<result name="success">/jsp/admin/agentsList.jsp</result>
			<result name="error">/jsp/admin/info.jsp</result>
            <interceptor-ref name="adminInterceptor"/>
            <interceptor-ref name="defaultStack" />
		</action>
		<action name="changeStatus" class="agentsManageAction" method="changeStatus">
			<result name="success">/jsp/admin/activityInfo.jsp</result>
		</action>
	</package>
	
